{
  "name": "archway-framework",
  "version": "1.0.1",
  "description": "Small but powerful UI centered framework",
  "type": "module",
  "main": "./dist/core/cjs/index.js",
  "module": "./dist/core/esm/index.js",
  "exports": {
    ".": {
      "require": "./dist/core/cjs/index.js",
      "default": "./dist/core/esm/index.js"
    }
  },
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx rollup -c",
    "dev": "npx rollup -c --watch --environment mode:dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/archway-team/archway-framework.git"
  },
  "keywords": [
    "archway",
    "frontend",
    "framework",
    "vdom",
    "ui"
  ],
  "author": "Nathan Pham <nathanpham.me@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/archway-team/archway-framework/issues"
  },
  "homepage": "https://github.com/archway-team/archway-framework#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "rollup": "^2.33.3",
    "rollup-plugin-terser": "^7.0.2"
  }
}
